@model IEnumerable<SchoolManagement.mvc.DataDB.StudentEnrollmentTable>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Co)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Co.CoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stu.StudentRegId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
